{
  "version": 2,
  "buildCommand": "CI=false next build --no-lint",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_ENABLE_LOGS": "false",
    "NEXT_PUBLIC_ENABLE_DEBUG": "false",
    "NEXT_PUBLIC_DISABLE_WARNINGS": "false"
  },
  "public": false
} 